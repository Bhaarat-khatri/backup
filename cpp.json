    "int MOD = 1e9 + 7;",
    "int power(int x, int y, int p = MOD) {",
    "  unsigned long long res = 1;",
    "  x = x % p;",
    "  while (y > 0) {",
    "    if (y & 1)",
    "      res = (res * x) % p;",
    "    y = y >> 1;",
    "    x = (x * x) % p;",
    "  }",
    "  return res;",
    "}",
    "// using fermats little thm. [p needs to be prime which",
    "// is mostly the case as mod value generally is 1e9+7]",
    "int modInverse(int n, int p = MOD) { return power(n, p - 2, p); }",
    "// can also derive this using extended euclidean... however this has a much",
    "int nCr(int n, int r, int p = MOD) {",
    "  if (n < r)",
    "    return 0;",
    "  if (r == 0)",
    "    return 1;",
    "  vector<int> fac(n + 1, 0);",
    "  fac[0] = 1;",
    "  for (int i = 1; i <= n; i++)",
    "    fac[i] = (fac[i - 1] * i) % p;",
    "  return (fac[n] * modInverse(fac[r], p) % p * modInverse(fac[n - r], p) % p) %",
