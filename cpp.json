		  "    void update(int idx,int val){     // v is the new value!",
		  "        update(idx,val,1,0,n-1);",
		  "    }",
		  "    void printTree(vector<int>& v){",
		  "        cout<<v<<\"\\n\";",
		  "    }",
		  "",
		  "};",
